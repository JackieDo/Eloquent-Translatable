{
    "name": "jackiedo/eloquent-translatable",
    "description": "Making Eloquent models translatable",
    "keywords": ["laravel", "translate", "translation", "multilingual", "language", "eloquent"],
    "license": "MIT",
    "authors": [
        {
            "name": "Jackie Do",
            "email": "anhvudo@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/console": "^5.3|^6.0|^7.0",
        "illuminate/database": "^5.3|^6.0|^7.0",
        "illuminate/support": "^5.3|^6.0|^7.0",
        "illuminate/validation": "^5.3|^6.0|^7.0",
        "illuminate/events": "^5.3|^6.0|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Jackiedo\\EloquentTranslatable\\": "src/Jackiedo/EloquentTranslatable"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jackiedo\\EloquentTranslatable\\EloquentTranslatableServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
